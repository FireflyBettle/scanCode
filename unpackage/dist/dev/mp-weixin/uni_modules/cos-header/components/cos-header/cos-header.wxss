
.cos-header {
	position: relative;
	width: 100vw;
}
.nav-bg {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	width: 100vw;
}
.is_fixed {
	position: fixed;
	top: 0;
	right: 0;
	left: 0;
	z-index: 99;
}
.nav-wrapper {
	position: relative;
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 0 8px;
}
.img {
	width: 20px;
	height: 20px;
	color: #fff;
}
.nav-back {
	flex-shrink: 0;
}
.nav-title {
	text-align: center;
	white-space: nowrap;
	overflow: hidden;
	word-break: break-all;
	text-overflow: ellipsis;
}
.nav-menu {
	flex-shrink: 0;
}

